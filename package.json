{
  "name": "videojs-annotation",
  "version": "4.0.0",
  "description": "Video.js plugin for timeline annotations, spatial shapes, and threaded comments using W3C Web Annotation format",
  "repository": {
    "type": "git",
    "url": "https://github.com/micahchoo/videojs-annotation.git"
  },
  "main": "build/videojs-annotation.cjs.js",
  "module": "build/videojs-annotation.esm.js",
  "exports": {
    ".": {
      "import": "./build/videojs-annotation.esm.js",
      "require": "./build/videojs-annotation.cjs.js",
      "default": "./build/videojs-annotation.esm.js"
    },
    "./react": {
      "import": "./build/videojs-annotation-react.esm.js",
      "default": "./build/videojs-annotation-react.esm.js"
    },
    "./css": "./build/css/annotations.css"
  },
  "sideEffects": [
    "./build/videojs-annotation.js",
    "./build/videojs-annotation.min.js",
    "./build/css/*.css"
  ],
  "style": "build/css/annotations.css",
  "files": [
    "build"
  ],
  "scripts": {
    "prebuild": "node scripts/compile-scss.js",
    "build": "rollup -c",
    "watch": "rollup -c -w",
    "serve": "node scripts/dev-server.js",
    "test": "mocha test/mocha/components/*.js",
    "lint": "eslint ./src"
  },
  "author": "Evan Carothers (@ecaroth) & Jack Pope (@jackpope)",
  "copyright": "Copyright Contently, Inc. <https://www.contently.com/>",
  "license": "Apache-2.0",
  "devDependencies": {
    "@babel/core": "^7.24.0",
    "@babel/eslint-parser": "^7.24.0",
    "@babel/preset-env": "^7.24.0",
    "@rollup/plugin-babel": "^6.0.0",
    "@rollup/plugin-commonjs": "^25.0.0",
    "@rollup/plugin-node-resolve": "^15.0.0",
    "@rollup/plugin-terser": "^0.4.0",
    "@semantic-release/changelog": "^3.0.5",
    "@semantic-release/git": "^7.0.18",
    "chai": "^4.2.0",
    "eslint": "^8.0.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-prettier": "^5.0.0",
    "mocha": "^10.0.0",
    "prettier": "^3.0.0",
    "rollup": "^4.0.0",
    "sass": "^1.70.0",
    "semantic-release": "^15.13.21",
    "video.js": "^7.6.5"
  },
  "peerDependencies": {
    "video.js": "^7 || ^8",
    "react": "^16.8.0 || ^17 || ^18 || ^19"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    }
  },
  "release": {
    "branch": "master",
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/changelog",
      "@semantic-release/npm",
      "@semantic-release/github",
      [
        "@semantic-release/git",
        {
          "assets": [
            "CHANGELOG.md",
            "package.json",
            "yarn.lock"
          ],
          "message": "chore(release): ${nextRelease.version} [skip ci]\n\n${nextRelease.notes}"
        }
      ]
    ]
  },
  "dependencies": {
    "mitt": "^1.2.0"
  }
}
